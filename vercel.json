{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "excludeFiles": "{.next,*.cache,node_modules,public,app,venv,__pycache__,*.pyc,*.pyo,*.pyd,tests,images,*.jpg,*.jpeg,*.png,*.gif}/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}
